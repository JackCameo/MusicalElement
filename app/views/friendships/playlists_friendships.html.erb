<h1>PlayLists for "#{friend.email}"</h1>

  <% @friend.playlists.each do |pl| %>
  <table id="draggable-<%= pl.id %>" class="ui-widget-content">
    <tr>
        <th><%= pl.name %></th>
        <th> Playlist: <%= rating_for pl, "playlist" %></th>
    </tr>

    <tr>
      <% pl.tracks.each do |t| %>
      <tr><td><%= t.title %></td></tr>


      <% end %>